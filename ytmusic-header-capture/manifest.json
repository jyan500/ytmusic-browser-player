{
    "manifest_version": 3,
    "name": "YT Music Header Collector",
    "version": "1.0",
    "description": "Collect headers from music.youtube.com",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://music.youtube.com/*"
    ],
    // "background": {
    //     "service_worker": "background.js"
    // },
    "action": {
        "default_popup": "js/index.html",
        "default_title": "React Chrome Extension"
    }

}